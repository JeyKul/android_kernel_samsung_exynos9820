		sx9330,avgbfilt_reg = <0x60600E00>;   /* AVGPOSFILT : 1/256, AVGNEGFILT : 1/2 */
		sx9330,avgafilt_reg = <0x00000000>;
		sx9330,advdig3_reg = <0x00000000>;
		sx9330,advdig4_reg = <0x00000000>;
		sx9330,refcorra_reg = <0x00000000>;
	};
};

